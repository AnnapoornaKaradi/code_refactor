 : error CS5001: Program does not contain a static 'Main' method suitable for an entry point [/src/BCOWebJobDailyOnce/BCOWebJobDailyOnce.csproj]


#See https://aka.ms/containerfastmode to understand how Visual Studio uses this Dockerfile to build your images for faster debugging.
FROM icsechgdacrn1.azurecr.io/dotnet-runtime:8-dev AS base
USER root

RUN mkdir /app && chmod -R 777 /app

## BUILD SECTION
FROM icsechgdacrn1.azurecr.io/dotnet-sdk:latest-dev AS builder
USER root
WORKDIR /src

COPY ["BCOWebJobDailyOnce.csproj", "BCOWebJobDailyOnce/"]
RUN dotnet restore "BCOWebJobDailyOnce/BCOWebJobDailyOnce.csproj"
COPY . .
WORKDIR "/src/BCOWebJobDailyOnce"
RUN dotnet build "BCOWebJobDailyOnce.csproj" -c Release -o /app/build
USER nonroot

## PUBLISH SECTION
FROM builder AS publish
USER root
RUN dotnet publish "BCOWebJobDailyOnce.csproj" -c Release -o /app/publish
USER nonroot

## IMAGE PUSH SECTION
FROM base AS final
WORKDIR /app
RUN apk add --no-cache  ca-certificates
ADD FNFPHXIssuingCA.crt /usr/local/share/ca-certificates/FNFPHXIssuingCA.crt
ADD FNFRootCA.crt /usr/local/share/ca-certificates/FNFRootCA.crt
ADD FNFProdGlobalRootCA.crt /usr/local/share/ca-certificates/FNFProdGlobalRootCA.crt
ADD FNFProdGlobalSubCA01.crt /usr/local/share/ca-certificates/FNFProdGlobalSubCA01.crt
ADD FNFProdGlobalSubCA02.crt /usr/local/share/ca-certificates/FNFProdGlobalSubCA02.crt
ADD FNFProdGlobalSub03.crt /usr/local/share/ca-certificates/FNFProdGlobalSub03.crt
RUN chmod 644 /usr/local/share/ca-certificates/FNFPHXIssuingCA.crt 
RUN chmod 644 /usr/local/share/ca-certificates/FNFRootCA.crt 
RUN chmod 644 /usr/local/share/ca-certificates/FNFProdGlobalRootCA.crt 
RUN chmod 644 /usr/local/share/ca-certificates/FNFProdGlobalSubCA01.crt 
RUN chmod 644 /usr/local/share/ca-certificates/FNFProdGlobalSubCA02.crt
RUN chmod 644 /usr/local/share/ca-certificates/FNFProdGlobalSub03.crt && update-ca-certificates
COPY --from=publish --chown=65532:65532 /app/publish .
USER root 
RUN apk del wget apk-tools git busybox

USER nonroot

ENTRYPOINT ["dotnet", "BCOWebJobDailyOnce.dll"] 











